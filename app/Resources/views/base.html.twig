<!-- app/Resources/views/base.html.twig -->
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />
        <title>{% block title %}Orion7{% endblock %} - Orion7</title>
        <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        {% block javascript %}
            <script src="{{ asset('js/jquery-1.8.3.min.js') }}"></script>
            <script src="{{ asset('js/jquery-ui-1.9.2.custom.min.js') }}"></script>
        {% endblock %}
        {% block stylesheets %}
            <link href="{{ asset('css/screen.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/orion7/jquery-ui-1.9.2.custom.min.css') }}" type="text/css" rel="stylesheet" />

        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <section id="wrapper">
            <header id="header">
            <div class="header_text">Hola</div>
                
                {% block navigation %}
                    <nav>
                        <ul class="navigation">
                            <li><a href="#">Home</a></li>
                            <li><a href="#">About</a></li>
                            <li><a href="#">Contact</a></li>
                        </ul>
                    </nav>
                {% endblock %}
                
                <div class"clear"></div>
            </header>
            <section class="main-col">
                {% block body %}{% endblock %}
            </section>
            
            <div id="footer">
                {% block footer %}
                    Orion 7 loguitos de Voto Joven y de VI y demas
                {% endblock %}
            </div>
        </section>

        {% block javascripts %}{% endblock %}
    </body>
</html>